%
\begin{Frame}{Intro : Static Compiler Versus Compilette}

  \begin{block}{Static or dynamic code generation for performance}
    \begin{center}
  \Image[3]{CompilerVersusCompilette/CompilerStatic.pdf} \hspace{2cm}  \Image[4]{CompilerVersusCompilette/CompilerDynamic.pdf}
    \end{center}
  \end{block}   

\vspace{-0.9cm}

  \begin{columns}[t]
    \begin{column}{5cm} % Colonne gauche
      \begin{block}{Static Compiler}
        \begin{itemize}
        \item Run once
        \item Does not know data set characteristics
        \item Slow compilation (even with JIT)
        \end{itemize}
      \end{block} 
    \end{column}
    \begin{column}{5cm} % Colonne gauche
      \begin{alertblock}{Compilette}
        \begin{itemize}
        \item Adapt on the fly
        \item Knowledge of the architecture
        \item Knowledge of the application
        \end{itemize}
      \end{alertblock} 
    \end{column}
  \end{columns}  
\end{Frame}

